{
    "name": "chandrantwins/laravel-permission-ui",
    "description": "Simple UI for spatie-laravel-permission",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "chandran nepolean",
            "email": "chandrantwins@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "spatie/laravel-permission": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Dsalinas\\LPUI\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Dsalinas\\LPUI\\LaravelPermissionUIServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
